 FROM SO, SOItem, Product ,uom, part
WHERE SO.ID = SOItem.SOID
AND soitem.productid = product.id
AND soitem.uomid = uom.id
AND part.id = product.partid AND SOItem.statusID IN (20,30,40)
AND SO.statusID IN (20, 25)AND SOItem.typeID = 10
AND UPPER(part.num) LIKE UPPER($P{partNum})

Order BY 2]]>
