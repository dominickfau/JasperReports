	<queryString>
		<![CDATA[CALL CreateList($P!{Count}, 'count');
CALL CreateList(($P!{StartAtLabel} - 1), 'start');
SELECT product.num AS "ProductName", product.description AS Description, Product.Price, 1 AS selectOrder, currency.symbol
FROM product, tmp_count
    LEFT JOIN currency ON currency.homeCurrency = 1
WHERE product.id > 0
AND product.activeflag = 1
AND UPPER(product.num) LIKE UPPER($P{productNum})

UNION ALL

SELECT '1', '', 0.0, 0, currency.symbol
FROM tmp_start
    LEFT JOIN currency ON currency.homeCurrency = 1

ORDER BY 1]]>
	</queryString>
