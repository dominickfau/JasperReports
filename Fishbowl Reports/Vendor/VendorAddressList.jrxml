	<queryString>
		<![CDATA[SELECT UPPER(SUBSTRING(address.name FROM 1 FOR 1)) AS firstltr, address.name AS name, address.address AS address,
    address.city AS City, address.zip as Zip,  COALESCE(stateconst.code,'') AS stateabbr, stateconst.name AS statename,
    COALESCE(countryconst.abbreviation, '') AS countryabbr, countryconst.name AS countryName, addresstype.name AS "Type",
    address.accountID, company.name AS company

FROM address
    JOIN addresstype ON (addresstype.id = address.typeid AND addresstype.name = $P{cbAddressType})
    JOIN vendor ON vendor.accountid = address.accountid
    LEFT JOIN countryconst ON countryconst.id = address.countryid
    LEFT JOIN stateconst ON stateconst.id = address.stateid
    JOIN company ON company.id = 1

WHERE UPPER(Coalesce(stateconst.code, '')) LIKE UPPER($P{State})
  AND UPPER(Coalesce(address.zip,'')) LIKE UPPER($P{ZipCode})
  AND vendor.activeflag IN ($P{ckShowActive}, $P{ckShowInactive})

ORDER BY 1, UPPER(address.name)]]>
	</queryString>
