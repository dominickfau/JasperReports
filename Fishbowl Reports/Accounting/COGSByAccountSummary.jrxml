	<queryString>
		<![CDATA[SELECT so.num AS SONum, soitem.solineitem AS SOLineNum, postsoitem.postedtotalcost AS TotalCost,
    product.num AS ProductNum, product.description AS ProductDescription,
    COALESCE(asaccount.name, 'Use Default') AS COGSAccount,
    postso.postdate, company.name AS company, currency.symbol

FROM soitem
    LEFT JOIN so ON soitem.soid = so.id
    LEFT JOIN product ON soitem.productid = product.id
    LEFT JOIN part ON product.partid = part.id
    LEFT JOIN asaccount ON part.cogsaccountid = asaccount.id
    JOIN company ON company.id = 1
    JOIN postsoItem ON soitem.id = postsoItem.soitemId
    JOIN postSo ON postsoItem.postsoID = postso.id
    LEFT JOIN currency ON currency.homeCurrency = 1

WHERE soitem.typeid IN (10,11,12,20,21,50,60,70,80)
    AND soitem.statusid IN ($P!{ckPartial},$P!{ckFulfilled},$P!{ckClosedShort},40)
    AND UPPER(product.num) LIKE UPPER($P{productNum})
    AND UPPER(coalesce(asaccount.name, 'Use Default')) LIKE UPPER($P{COGSAccount})
    AND postso.postDate BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

ORDER BY asaccount.name, so.num]]>
	</queryString>
