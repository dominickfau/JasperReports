	<queryString>
		<![CDATA[SELECT post.amount AS amount, post.datecreated AS datecreated, post.dateposted AS dateposted,
    post.quantity AS qty, part.num AS part, post.refitemid AS type, uom.code AS uomcode,
    company.name AS company, currency.symbol, inventorylog.info AS notes

FROM post
    INNER JOIN part ON post.refid = part.id
    INNER JOIN uom ON uom.id = part.uomid
    INNER JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1
    LEFT JOIN inventorylog ON inventorylog.recordid = post.id AND inventorylog.tableid = -1407594912
WHERE post.typeid = 50
AND post.datecreated BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
AND post.refitemid IN ($P!{ckAddInitialInv},$P!{ckScrap},$P!{ckCycleCount},$P!{ckCostChange})
AND UPPER(part.num) LIKE UPPER($P{partNum})]]>
	</queryString>
