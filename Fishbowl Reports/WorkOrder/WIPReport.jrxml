	<queryString>
		<![CDATA[SELECT part.num AS "Part #", part.description AS "Description", wo.num AS "WO #", sum(COALESCE(pickitem.qty,woitem.qtytarget)) AS "Wo Qty",
    COALESCE(partcost.avgcost,0) AS "Avg Cost", part.stdcost AS "Standard Cost", wo.datescheduled AS "WO Date",
    tag.qty AS "Location Qty", location.name AS "Location", locationgroup.name AS "Location Group",
    uom.code as uomcode, company.name as company, wouom.code as wouom, location.id AS locationid, totalset.totalcostcheck

FROM tag
    JOIN part on tag.partid = part.id
    JOIN location on tag.locationid = location.id AND location.typeid = 80
    JOIN woitem on part.id = woitem.partid
    JOIN wo on woitem.woid = wo.id AND wo.locationid = location.id
    LEFT JOIN partcost on part.id = partcost.partid
    JOIN locationgroup on location.locationgroupid = locationgroup.id
    JOIN pickitem on woitem.id = pickitem.woitemid AND pickitem.statusid = 40 AND pickitem.partid = part.id
    JOIN UOM on part.uomid = uom.id
    JOIN company on company.id = 1
    JOIN uom wouom on woitem.uomid = wouom.id
    LEFT JOIN (SELECT SUM(tag.qty * IF($P{cbCostingMethod} = "Average Cost", COALESCE(partcost.avgcost,0),part.stdcost)) AS totalcostcheck
		FROM tag
		JOIN part ON tag.partid = part.id AND UPPER(part.num) LIKE UPPER($P{partNum})
		LEFT JOIN partcost ON part.id = partcost.partid
		JOIN location ON tag.locationid = location.id AND location.typeid = 80 AND location.id LIKE $P{locationID}
		JOIN (SELECT DISTINCT woitem.partid, wo.locationid
			FROM woitem
			JOIN wo ON woitem.woid = wo.id AND wo.statusid < 40
			JOIN pickitem ON woitem.id = pickitem.woitemid AND pickitem.statusid = 40
			) AS wos ON wos.partid = part.id AND wos.locationid = location.id
		JOIN locationgroup ON location.locationgroupid = locationgroup.id AND locationgroup.id IN ($P!{locationGroupID})
		) AS totalset ON 1 = 1


WHERE UPPER(part.num) LIKE UPPER($P{partNum})
  AND locationgroup.id IN ($P!{locationGroupID})
  AND location.id LIKE $P{locationID}
  AND wo.statusid < 40

GROUP BY part.num, part.description, wo.num, partcost.avgcost, part.stdcost,
    wo.datescheduled, location.name, locationgroup.name, tag.qty, uom.code,
    company.name, wouom.code

ORDER BY part.num, wo.num, locationgroup.name, location.name]]>
	</queryString>
