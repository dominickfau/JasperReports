	<parameter name="ReportTitle" class="java.lang.String" isForPrompting="false"/>
	<parameter name="salesPersonAssignedUser" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
    moitem.description AS WODESCRIPTION,
    wo.num AS WONUM,
    IF(bomitem.typeid = 40, bomitem.description, part.num) AS ITEMNUM,
    IF(bomitem.typeid = 40, '', part.description) AS ITEMDESCRIPTION,
    wo.qtytarget AS WOTARGET,
    woitem.qtytarget AS ITEMTARGET,
    bomitemtype.name AS ITEMTYPE,
    wo.datescheduled AS DATESCHEDULED,
    part.typeid AS PARTTYPE,
    COALESCE((
        SELECT
            SUM(QtyAllocated.qty)
        FROM
            QtyAllocated
        WHERE
            QtyAllocated.partID = Part.id
            AND qtyallocated.locationgroupid IN ($P!{locationGroupID})
            )
        , 0) AS QTYALLOCATED,
    COALESCE((
        SELECT
            SUM(qtyonhand.qty)
        FROM
            qtyonhand
        WHERE
            qtyonhand.partid = part.id
            AND qtyonhand.locationgroupid IN ($P!{locationGroupID})
            )
        , 0) AS QTYONHAND,
    COALESCE((
        SELECT
            SUM(qtynotavailable.qty)
        FROM
            qtynotavailable
        WHERE
            qtynotavailable.partid = part.id
            AND qtynotavailable.locationgroupid IN ($P!{locationGroupID})
            )
        , 0) AS QTYNOTAVAILABLE,
    COALESCE((
        SELECT
            SUM(qtydropship.qty)
        FROM
            qtydropship
        WHERE
            qtydropship.partid = part.id
            AND qtydropship.locationgroupid IN ($P!{locationGroupID})
            )
        , 0) AS QTYDROPSHIP,
    company.name AS COMPANY,
    COALESCE(uomconversion.factor, 1) AS FACTOR,
    COALESCE(uomconversion.multiply, 1) AS MULTIPLY,
    uom.code AS ITEMUOM,
    mo.num AS MONUM,
    part.id AS PARTID,
    calcategory.name AS calcat,
    IF(calcategory.color IS NOT NULL, CONCAT("<style backcolor='#",calcategory.color,"'><style forecolor='#",calcategory.color,"'>"), '') AS catcolor

FROM
    moitem
    LEFT JOIN mo ON mo.id = moitem.moid
    LEFT JOIN woitem ON woitem.moitemid = moitem.id
    LEFT JOIN wo ON wo.id = woitem.woid
    LEFT JOIN bomitem ON bomitem.id = moitem.bomitemid
    LEFT JOIN bomitemtype ON bomitemtype.id = moitem.typeid
    LEFT JOIN part ON woitem.partid = part.id
    LEFT JOIN uom ON part.uomid = uom.id
    LEFT JOIN uomconversion ON uomconversion.touomid = part.uomid
        AND uomconversion.fromuomid = woitem.uomid
    INNER JOIN company ON company.id = 1
    INNER JOIN location ON wo.locationid = location.id
        AND location.pickable = 1
    LEFT JOIN calcategory ON wo.calcategoryid = calcategory.id
    	LEFT JOIN	(
		SELECT GROUP_CONCAT(sysuser.username) AS users, woassignedusers.woid
		FROM sysuser
		LEFT JOIN woassignedusers ON sysuser.id = woassignedusers.userid
		GROUP BY woassignedusers.woid
		) AS wouser ON wo.id = wouser.woid

WHERE
    wo.statusid IN (10,20,30)
    AND wo.num LIKE $P{woNum}
    AND wo.datescheduled BETWEEN $P{dateRange1}
        AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
    AND wo.locationgroupid IN ($P!{locationGroupID})
    AND COALESCE(calcategory.name,'') LIKE $P{CalendarCategory}
    AND COALESCE(wouser.users,'') LIKE (CONCAT('%',$P{salesPersonAssignedUser},'%'))

ORDER BY $P!{sort}, wo.num, bomitemtype.name, itemnum]]>
	</queryString>
