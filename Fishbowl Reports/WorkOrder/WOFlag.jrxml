	<subDataset name="POs" uuid="ae21785f-9ecd-4704-aff3-d717efa87d03">
		<parameter name="partID" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[Select po.num as num, poitem.partid as partid, poitem.qtytofulfill - poitem.qtyfulfilled as qty, uom.code as uom
                      from po
                      JOIN poitem on po.id = poitem.poid
                      JOIN uom on poitem.uomid = uom.id
                      where po.statusid < 60 and po.statusid > 10
                      AND poitem.statusid < 50
                      AND poitem.partid  = $P{partID}]]>
		</queryString>
		<field name="NUM" class="java.lang.String"/>
		<field name="PARTID" class="java.lang.Integer"/>
		<field name="QTY" class="java.lang.Double"/>
		<field name="UOM" class="java.lang.String"/>
		<variable name="QtyDecimal" class="java.lang.String" resetType="None">
			<variableExpression><![CDATA[System.getProperty("REPORT_DECIMAL_QTY")]]></variableExpression>
		</variable>
	</subDataset>
