	<queryString>
		<![CDATA[SELECT wo.num AS wonumber, company.name AS company, wo.id AS woid, wo.datescheduled,
    wo.datestarted AS datestarted, wo.datefinished AS datefinished

FROM wo
    JOIN company ON company.id = 1

WHERE wo.statusid LIKE $P{cbWOStatus}
  AND wo.locationgroupid IN ($P!{locationGroupID})
  AND wo.num LIKE $P{woNum}
  AND $P!{cbApplyDateRangeTo} BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

ORDER BY wo.num]]>
	</queryString>
