	<queryString>
		<![CDATA[SELECT memo.datecreated AS datecreated, memo.username AS username, memo.memo AS memo,
    company.name AS company

FROM memo
    INNER JOIN tablereference ON memo.tableid = tablereference.tableid
    INNER JOIN company ON company.id = 1

WHERE tablereference.tablerefname = $P{TableName}
  AND memo.recordid = $P{RecordID}]]>
