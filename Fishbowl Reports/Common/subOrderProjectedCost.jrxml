	<variable name="Cost" class="java.lang.Double" resetType="None">
		<variableExpression><![CDATA[$F{WOSTATUSID} == 40 ?
    $V{FinalCost} :
    ($F{PARTUOMID} == $F{WOITEMUOMID} ?
        $V{FinalCost} :
        ($V{FinalCost} * $F{PARTUOMMULTIPLY}) / $F{PARTUOMFACTOR})]]></variableExpression>
		<initialValueExpression><![CDATA[]]></initialValueExpression>
	</variable>
	<variable name="FinalCost" class="java.lang.Double" resetType="None">
		<variableExpression><![CDATA[$F{WOSTATUSID} == 40 ?
    ($F{WOITEMQTYUSED} == 0 ?
        0 :
        (($F{WOITEMCOST} / ($F{WOITEMQTYUSED} * $F{PARTUOMMULTIPLY} / $F{PARTUOMFACTOR})) * $F{ITEMUOMMULTIPLY} / $F{ITEMUOMFACTOR})) :
    ($V{CostingMethod}.equals("Average Costing") ?
    $F{AVGCOST} :
    $V{CostingMethod}.equals("Standard Costing") ?
        $F{STDCOST} :
        $V{CostingMethod}.equals("FIFO Costing") ?
            $F{FIFOCOST} :
            $F{LIFOCOST})]]></variableExpression>
	</variable>
	<variable name="QtyNeededForOrder" class="java.lang.Double">
		<variableExpression><![CDATA[$F{QTYORDERED} * $F{WOITEMTARGET}]]></variableExpression>
	</variable>
	<variable name="CostingMethod" class="java.lang.String" resetType="None">
		<variableExpression><![CDATA[System.getProperty("Acct Costing Method")]]></variableExpression>
	</variable>
	<variable name="TotalCost" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[!$F{ITEMTYPE}.equals("Finished Good") ?
    $F{WOSTATUSID} == 40 ?
        $F{WOITEMCOST} :
        ($F{PARTUOMID} == $F{WOITEMUOMID} ?
            $V{FinalCost} :
            ($V{FinalCost} * $F{PARTUOMMULTIPLY}) / $F{PARTUOMFACTOR}) * $F{WOITEMTARGET} :
        0]]></variableExpression>
	</variable>
