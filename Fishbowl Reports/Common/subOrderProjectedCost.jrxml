	<queryString>
		<![CDATA[SELECT MO.ID AS MOID, MO.NUM AS MONUM, MO.DATESCHEDULED AS MODATESCHEDULED, MOSTATUS.NAME AS MOSTATUS,
    MOLG.NAME AS MOLG, MO.DATEISSUED, MO.DATECOMPLETED, MO.NOTE AS MONOTE, WO.NUM AS WONUM, QBCLASS.NAME AS WOCLASS,
    WO.DATESTARTED AS WODATESTARTED, WO.DATEFINISHED AS WODATEFINISHED, UOM.CODE AS UOMCODE, WOITEMUOM.CODE AS WOITEMUOM,
    WOSTATUS.NAME AS WOSTATUS, BOM.NUM AS BOMNUM, WO.QTYORDERED, WOLG.NAME || ' - ' || WOLOC.NAME AS WOLOCATION,
    PART.NUM AS WOITEMPART, WOITEM.QTYTARGET AS WOITEMNEEDED, COALESCE(ONHAND.QTY,0) AS QTYONHAND,
    WOUSERS.USERS AS WOUSERS, PART.DESCRIPTION AS WOITEMDESCRIPTION, COMPANY.NAME AS COMPANY, PART.TYPEID AS PARTTYPEID,
    COALESCE(PARTUOMCONV.FACTOR,1) AS PARTUOMFACTOR, COALESCE(PARTUOMCONV.MULTIPLY,1) AS PARTUOMMULTIPLY,
    COALESCE(ITEMUOMCONV.FACTOR,1) AS ITEMUOMFACTOR, COALESCE(ITEMUOMCONV.MULTIPLY,1) AS ITEMUOMMULTIPLY,
    COALESCE(WOITEM.COST,0) AS WOITEMCOST, COALESCE(WOITEM.QTYTARGET,0) AS WOITEMTARGET, BOMITEMTYPE.NAME AS ITEMTYPE,
    PART.UOMID AS PARTUOMID, WOITEM.UOMID AS WOITEMUOMID, WO.STATUSID AS WOSTATUSID, WOITEM.QTYUSED AS WOITEMQTYUSED,
    (CASE PART.TYPEID WHEN 10 THEN COALESCE(PARTCOST.AVGCOST, 0) ELSE COALESCE(PART.STDCOST, 0) END) AS AVGCOST, COALESCE(PART.STDCOST, 0) AS STDCOST, CURRENCY.SYMBOL,
    COALESCE(COALESCE((CASE PART.TYPEID WHEN 10 THEN ((SELECT (COALESCE(COSTLAYER.ORGTOTALCOST,0) / COSTLAYER.ORGQTY) AS COST
                 FROM COSTLAYER
                WHERE COSTLAYER.PARTID = PART.ID
                  AND COSTLAYER.STATUSID = 10
                ORDER BY COSTLAYER.ID ASC LIMIT 1)) ELSE COALESCE(PART.STDCOST, 0) END),
		(CASE PART.TYPEID WHEN 10 THEN ((SELECT (COALESCE(COSTLAYER.ORGTOTALCOST,0) / COSTLAYER.ORGQTY) AS COST
                 FROM COSTLAYER
                WHERE COSTLAYER.PARTID = PART.ID
                ORDER BY COSTLAYER.DATELASTMODIFIED DESC LIMIT 1)) ELSE COALESCE(PART.STDCOST, 0) END)),0) AS FIFOCOST,
    COALESCE(COALESCE((CASE PART.TYPEID WHEN 10 THEN ((SELECT (COALESCE(COSTLAYER.ORGTOTALCOST,0) / COSTLAYER.ORGQTY) AS COST
                 FROM COSTLAYER
                WHERE COSTLAYER.PARTID = PART.ID
                  AND COSTLAYER.STATUSID = 10
                ORDER BY COSTLAYER.ID DESC LIMIT 1)) ELSE COALESCE(PART.STDCOST, 0) END),
		(CASE PART.TYPEID WHEN 10 THEN ((SELECT (COALESCE(COSTLAYER.ORGTOTALCOST,0) / COSTLAYER.ORGQTY) AS COST
                 FROM COSTLAYER
                WHERE COSTLAYER.PARTID = PART.ID
                ORDER BY COSTLAYER.DATELASTMODIFIED DESC LIMIT 1)) ELSE COALESCE(PART.STDCOST, 0) END)),0) AS LIFOCOST

FROM mo
    INNER JOIN moitem ON mo.id = moitem.moid
    INNER JOIN mostatus ON mo.statusid = mostatus.id
    INNER JOIN locationgroup AS molg ON mo.locationgroupid = molg.id
    LEFT JOIN wo ON moitem.id = wo.moitemid
    LEFT JOIN qbclass ON qbclass.id = wo.qbclassid
    LEFT JOIN wostatus ON wo.statusid = wostatus.id
    LEFT JOIN locationgroup AS wolg ON wo.locationgroupid = wolg.id
    LEFT JOIN location AS woloc ON wo.locationid = woloc.id
    LEFT JOIN woitem ON wo.id = woitem.woid AND woitem.typeid IN (10,20,30)
    LEFT JOIN part ON woitem.partid = part.id
    LEFT JOIN partcost ON woitem.partid = partcost.partid
    LEFT JOIN uom ON part.uomid = uom.id
    LEFT JOIN uom AS woitemuom ON woitem.uomid = woitemuom.id
    LEFT JOIN uomconversion AS partuomconv ON (partuomconv.touomid = part.uomid AND partuomconv.fromuomid = woitem.uomid)
    LEFT JOIN uomconversion AS itemuomconv ON (itemuomconv.touomid = part.uomid AND itemuomconv.fromuomid = woitem.uomid)
    LEFT JOIN bom ON moitem.bomid = bom.id
    LEFT JOIN bomitemtype ON woitem.typeid = bomitemtype.id
    LEFT JOIN (SELECT qtyonhand.partid, SUM(qtyonhand.qty) AS qty
        FROM qtyonhand GROUP BY qtyonhand.partid) onhand ON onhand.partid = woitem.partid
    LEFT OUTER JOIN (SELECT woassignedusers.woid, GROUP_CONCAT(sysuser.username, ', ') AS users
        FROM woassignedusers
            INNER JOIN sysuser ON woassignedusers.userid = sysuser.id
        GROUP BY woassignedusers.woid) wousers ON wousers.woid = wo.id
    INNER JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1

WHERE wo.id LIKE $P{woID}
  AND (moitem.bomid IS NOT NULL OR woitem.partid IS NOT NULL)
  AND wo.qtyordered IS NOT NULL

ORDER BY wo.num, wo.datescheduled, woitem.typeid, woitem.sortid, woitem.description]]>
	</queryString>
