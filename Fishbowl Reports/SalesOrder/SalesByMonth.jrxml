	<queryString>
		<![CDATA[SELECT ((POSTSOITEM.QTY * COALESCE(UOMCONVERSION.MULTIPLY,1)) / COALESCE(UOMCONVERSION.FACTOR,1)) AS POSTSOITEMQTY, POSTSO.POSTDATE AS POSTSOPOSTDATE,
    PRODUCT.NUM AS PRODUCTNUM, PRODUCT.DESCRIPTION AS PRODUCTDESCRIPTION,
    ((SOITEM.UNITPRICE + (SOITEM.ADJUSTAMOUNT / (CASE WHEN SOITEM.QTYTOFULFILL = 0
                                                            THEN 1
                                                            ELSE SOITEM.QTYTOFULFILL END))) * POSTSOITEM.QTY) AS TOTALPRICE,
    uom.code AS UOMCODE, company.name AS COMPANY, currency.symbol AS SYMBOL

FROM postsoitem
    JOIN soitem ON postsoitem.soitemid = soitem.id
    JOIN product ON soitem.productid = product.id
    JOIN uom ON product.uomid = uom.id
    LEFT JOIN uomconversion ON (uomconversion.touomid = product.uomid AND uomconversion.fromuomid = soitem.uomid)
    JOIN company ON company.id = 1
    LEFT JOIN postso ON postso.id = postsoitem.postsoid
    LEFT JOIN currency ON currency.homecurrency = 1

WHERE product.num LIKE $P{productNum}
  AND postso.postdate BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
  AND soitem.typeid IN (10,12,80)

ORDER BY 2]]>
	</queryString>
