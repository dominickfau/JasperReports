	<queryString>
		<![CDATA[SELECT part.num AS partnum, part.description AS description, so.billtoname AS bname, so.num AS sonum,
    soitem.datescheduledfulfillment AS datetofulfill, COALESCE((soitem.qtytofulfill-soitem.qtyfulfilled), 0) AS quantityneeded,
    soitem.qtyfulfilled AS qtyfulfilled, soitem.solineitem, uom.code AS uom, uomconversion.factor AS uomfactor,
    uomconversion.multiply AS uommulti, part.id AS partid,
    COALESCE((SELECT SUM(qtyonhand.qty)
                FROM qtyonhand
               WHERE qtyonhand.partid = part.id
                 AND qtyonhand.locationGroupID IN ($P!{locationGroupID})), 0) AS qty,
    COALESCE((SELECT SUM(QtyAllocated.qty)
                FROM QtyAllocated
               WHERE QtyAllocated.partID = Part.id
                 AND qtyallocated.locationGroupID IN ($P!{locationGroupID})), 0) AS allocated,
    COALESCE((SELECT SUM(qtynotavailable.qty)
                FROM qtynotavailable
               WHERE qtynotavailable.partid = part.id
                 AND qtynotavailable.locationGroupID IN ($P!{locationGroupID})), 0) AS Unavailable,
    COALESCE((SELECT SUM(qtydropship.qty)
                FROM qtydropship
               WHERE qtydropship.partid = part.id
                 AND qtydropship.locationGroupID IN ($P!{locationGroupID})), 0) AS DropShip

FROM so
	JOIN soitem ON (so.id = soitem.soid AND soitem.typeid = 10 AND so.locationGroupID IN ($P!{locationGroupID}))
	LEFT JOIN product ON product.id = soitem.productid
	LEFT JOIN part ON part.id = product.partid
	INNER JOIN uom ON part.uomid = uom.id
	LEFT JOIN (SELECT tag.qty AS tagqty, tag.qtycommitted AS tagqtycommitted, tag.partid AS tagpartid, location.id AS locationid, locationgroupid AS locationgroupid
		FROM tag
		LEFT JOIN location ON (tag.locationid = location.id AND location.activeflag = 1 AND location.pickable = 1 AND location.countedasavailable = 1)
		LEFT JOIN locationGroup ON (location.locationgroupid = locationgroup.id AND locationGroup.id IN ($P!{locationGroupID}) OR locationgroup.id IS NULL)) tags ON (tags.locationgroupid IN ($P!{locationGroupID}) AND tags.tagpartid = part.id)
	LEFT JOIN uomconversion ON (part.uomid = uomconversion.touomid AND soitem.uomid = uomconversion.fromuomid)

WHERE so.statusid IN ($P{ckSOEstimate},$P{ckSOIssued},$P{ckSOInProgress})
  AND part.typeid IN (10)
  AND UPPER(so.num) LIKE UPPER($P{soNum})
  AND UPPER(product.num) LIKE UPPER($P{productNum})
  AND so.customerid LIKE $P{customerID}
  AND soitem.datescheduledfulfillment BETWEEN $P{dateRange1} AND $P{dateRange2}

GROUP BY partnum, description, bname, sonum, datetofulfill, quantityneeded, qtyfulfilled, soitem.solineitem,
    uom, uomfactor, uommulti,part.id

ORDER BY $P!{sort}]]>
	</queryString>
