		<parameter name="suggestedStart" class="java.util.Date" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
    part.num as partNum,
    coalesce(tag.qty,0) as onHand,
    sum(soitem.qtytofulfill) as qtyOrdered,
    coalesce(tag.qty,0) - sum(soitem.qtytofulfill) as qtyShort,
    date_sub(so.datefirstship, interval coalesce(bom.estimatedduration,0) minute) as suggestedStart

FROM soitem

    INNER JOIN company ON company.id = 1
    INNER JOIN so ON soitem.soid = so.id
    INNER JOIN customer ON so.customerid = customer.id
    INNER JOIN locationgroup ON so.locationgroupid = locationgroup.id

    INNER JOIN product ON soitem.productid = product.id
    INNER JOIN uom ON soitem.uomid = uom.id
    LEFT OUTER JOIN part ON product.partid = part.id
    LEFT OUTER JOIN bomitem ON part.id = bomitem.partid AND bomitem.typeid = 10 AND bomitem.bomid = part.defaultbomid
    LEFT OUTER JOIN bom ON bomitem.bomid = bom.id
    LEFT JOIN (SELECT SUM(tag.qty) AS QTY, tag.partid
                FROM tag
                JOIN location ON tag.locationid = location.id
                GROUP BY 2) AS Tag ON part.id = tag.partid

WHERE so.statusid < 60
  AND COALESCE(tag.qty,0) - soitem.qtytofulfill < 0
  AND so.datecreated BETWEEN $P{dateRange1} AND date_sub($P{dateRange2}, interval 1 second)
  AND part.num LIKE '$P!{partNum}'
  AND so.num LIKE '$P!{soNum}'
  AND locationgroup.id IN ($P!{locationGroupID})
  AND COALESCE(customer.id,0) LIKE '$P!{customerID}'
  AND bom.id is not null

GROUP BY part.id
ORDER BY part.num]]>
		</queryString>
