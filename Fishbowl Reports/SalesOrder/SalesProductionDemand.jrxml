		</parameter>
		<queryString>
			<![CDATA[select distinct
    if(coalesce(stage4items.typeid, stage3items.typeid, stage2items.typeid) = 10,
    null,
    coalesce(stage4items.id, stage3items.id, stage2items.id)) as stageid,
    stage4items.id as stage4id,
    stage3items.id as stage3id,
    stage2items.id as stage2id,

    coalesce(stage4part.num,stage3part.num,stage2part.num,part.num) as partNumber,
    coalesce(stage4part.description,stage3part.description,stage2part.description,part.description) as partDescription,
    coalesce(stage4items.quantity,stage3items.quantity,stage2items.quantity,bomitem.quantity) as needed,
    case
        when stage4items.id is not null then coalesce(stage4tag.qty,0)
        when stage3items.id is not null then coalesce(stage3tag.qty,0)
        when stage2items.id is not null then coalesce(stage2tag.qty,0)
        else coalesce(tag.qty,0)
    end as onHand,
    uom.code as partUOM,
    (SELECT
            min(so.datefirstship)

        FROM soitem

            INNER JOIN so ON soitem.soid = so.id
            INNER JOIN customer ON so.customerid = customer.id
            INNER JOIN locationgroup ON so.locationgroupid = locationgroup.id

            INNER JOIN product ON soitem.productid = product.id
            INNER JOIN uom ON soitem.uomid = uom.id
            LEFT OUTER JOIN part tempPart ON product.partid = tempPart.id
            LEFT OUTER JOIN bomitem ON tempPart.id = bomitem.partid AND bomitem.typeid = 10 AND bomitem.bomid = tempPart.defaultbomid
            LEFT OUTER JOIN bom ON bomitem.bomid = bom.id
            LEFT JOIN (SELECT SUM(tag.qty) AS QTY, tag.partid, locationgroup.id AS locationGroupId
                        FROM tag
                        JOIN location ON tag.locationid = location.id
                        JOIN locationgroup ON location.locationgroupid = locationgroup.id
                        GROUP BY 2,3) AS Tag ON tempPart.id = tag.partid AND so.locationgroupid = tag.locationgroupid

        WHERE so.statusid < 60
        AND COALESCE(tag.qty,0) - soitem.qtytofulfill < 0
        AND so.datecreated BETWEEN $P{dateRange1} AND date_sub($P{dateRange2}, interval 1 second)
        AND tempPart.num LIKE '$P!{partNum}'
        AND so.num LIKE '$P!{soNum}'
        AND locationgroup.id IN ($P!{locationGroupID})
        AND COALESCE(customer.id,0) LIKE '$P!{customerID}'
        AND bom.id is not null
        AND tempPart.defaultbomid = coalesce(bomitem.bomid,stage2items.bomid,stage3items.bomid,stage4items.bomid)) as startDate,
        (select sum(estimatedduration) * 60000 from bom where (id = bomitem.bomid OR id = stage4items.bomid OR id = stage3items.bomid OR stage2items.bomid)) as bomTime

from bomitem
JOIN bom on bom.id = bomitem.bomid
JOIN part on bomitem.partid = part.id
JOIN uom on part.uomid = uom.id
LEFT JOIN uomconversion ON (uomconversion.touomid = part.uomid AND uomconversion.fromuomid = bomitem.uomid)
LEFT JOIN (SELECT SUM(tag.qty) as qty, tag.partid, location.locationgroupid AS locationgroupid
               from tag
	     JOIN location ON tag.locationid = location.id
               group by 2,3) as tag on bomitem.partid = tag.partid and tag.locationgroupid IN ($P!{locationGroupID})

left join bomitem stage2items on stage2items.bomid = bomitem.stagebomid
left join part stage2part on stage2part.id = stage2items.partid
LEFT JOIN (SELECT SUM(tag.qty) as qty, tag.partid, location.locationgroupid AS locationgroupid
               from tag
	     JOIN location ON tag.locationid = location.id
               group by 2,3) as stage2tag on stage2items.partid = stage2tag.partid and stage2tag.locationgroupid IN ($P!{locationGroupID})

left join bomitem stage3items on stage3items.bomid = stage2items.stagebomid
left join part stage3part on stage3part.id = stage3items.partid
LEFT JOIN (SELECT SUM(tag.qty) as qty, tag.partid, location.locationgroupid AS locationgroupid
               from tag
	     JOIN location ON tag.locationid = location.id
               group by 2,3) as stage3tag on stage3items.partid = stage3tag.partid and stage3tag.locationgroupid IN ($P!{locationGroupID})

left join bomitem stage4items on stage4items.bomid = stage3items.stagebomid
left join part stage4part on stage4part.id = stage4items.partid
LEFT JOIN (SELECT SUM(tag.qty) as qty, tag.partid, location.locationgroupid AS locationgroupid
               from tag
	     JOIN location ON tag.locationid = location.id
               group by 2,3) as stage4tag on stage4items.partid = stage4tag.partid and stage4tag.locationgroupid IN ($P!{locationGroupID})

where bomitem.typeid not in (10,31)
AND bom.id IN($P!{bomList})
AND part.typeid = 10
AND coalesce(stage2part.typeid,10) = 10
AND coalesce(stage3part.typeid,10) = 10
AND coalesce(stage4part.typeid,10) = 10

group by stage4items.id, stage3items.id, stage2items.id, bomitem.id

having onhand - needed < 0

order by partNumber,startDate]]>
