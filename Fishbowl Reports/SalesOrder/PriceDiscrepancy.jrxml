	<queryString>
		<![CDATA[SELECT distinct soitem.productnum AS ProductNum, soitem.unitprice + (soitem.adjustamount / (CASE WHEN soitem.qtytofulfill = 0
                                                            THEN 1
                                                            ELSE soitem.qtytofulfill END)) AS UnitPrice, product.price as ProductPrice,
    so.num AS SONum, so.salesman as UserName, so.dateissued AS DateIssued, soitem.solineitem AS LineItem,
    soitemstatus.name as ItemStatus, customer.name AS CustomerName, company.name AS company,
    (CASE WHEN COALESCE(iskititem.id,0) = 0 THEN 0 ELSE 1 END) AS iskititem,
    (CASE WHEN COALESCE(iskitheader.id,0) = 0 THEN 0 ELSE 1 END) AS iskitheader, currency.symbol

FROM soitem
    LEFT JOIN so ON so.id = soitem.soid
    LEFT JOIN product ON soitem.productid = product.id
    LEFT JOIN customer ON customer.id = so.customerid
    LEFT JOIN soitemstatus ON soitem.statusid = soitemstatus.id
    LEFT JOIN objecttoobject AS iskititem ON (soitem.id = iskititem.recordid1 AND iskititem.tableid1 = 1002104832)
    LEFT JOIN objecttoobject AS iskitheader ON (soitem.id = iskitheader.recordid2 AND iskitheader.tableid2 = 1002104832)
    JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1

WHERE soitem.typeid in (10,12,80)
  AND so.dateissued BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
  AND soitem.unitprice <> product.price
  AND so.customerid LIKE $P{customerID}
  AND UPPER(so.num) LIKE UPPER($P{soNum})
  AND UPPER(so.salesman) LIKE UPPER($P{salesPerson})]]>
	</queryString>
