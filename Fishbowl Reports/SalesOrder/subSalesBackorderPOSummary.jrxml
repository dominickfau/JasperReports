	<parameter class="java.lang.String" isForPrompting="false" name="partID"/>
	<queryString>
		<![CDATA[SELECT part.num AS partnum, po.num AS ponum, (poitem.qtytofulfill - poitem.qtyfulfilled) AS qty,
    COALESCE(uomconversion.factor, 1) AS uomfactor, COALESCE(uomconversion.multiply, 1) AS uommultiply,
    poitem.datescheduledfulfillment

FROM po
    LEFT JOIN poitem ON po.id = poitem.poid
    LEFT JOIN part ON poitem.partid = part.id
    LEFT JOIN uomconversion ON (part.uomid = uomconversion.touomid AND poitem.uomid = uomconversion.fromuomid)

WHERE poitem.statusid IN (10, 20, 30, 40)
  AND poitem.typeid = 10
  AND poitem.partid LIKE $P{partID}
  AND po.locationgroupid IN ($P!{locationGroupID})]]>
	</queryString>
