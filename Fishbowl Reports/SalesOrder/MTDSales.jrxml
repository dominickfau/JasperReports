	<queryString>
		<![CDATA[CALL CREATELIST(31, 'createlist');
SELECT COALESCE(sales.qty, 0) AS qty, COALESCE(sales.amt,0) AS amt, cl.cnt AS dayofmonth, company.name AS company, currency.symbol

FROM tmp_CREATELIST AS CL
    LEFT JOIN (SELECT SUM(postsoitem.qty) AS qty, SUM(postsoitem.qty * (soitem.unitprice + (soitem.adjustamount / (CASE WHEN soitem.qtytofulfill = 0
                                                            						       THEN 1
                                                            					             ELSE soitem.qtytofulfill END)))) AS amt,
                   Year(postso.postdate) AS dateyear,
                   Month(postso.postdate) AS datemonth,
                   Day(postso.postdate) AS dateday
                 FROM postsoitem
		 JOIN postso ON postso.id = postsoitem.postsoid
                     LEFT JOIN soitem ON soitem.id = postsoitem.soitemid
                GROUP BY dateyear, datemonth, dateday) sales ON (sales.dateyear = Year(CAST(DATE_SUB($P{Date}, INTERVAL 1 SECOND) as date))
                                                             AND sales.datemonth = Month(CAST(DATE_SUB($P{Date}, INTERVAL 1 SECOND) as date))
                                                             AND sales.dateday = cl.cnt)
    LEFT JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1]]>
	</queryString>
