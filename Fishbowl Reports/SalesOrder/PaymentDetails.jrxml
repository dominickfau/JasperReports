	<queryString>
		<![CDATA[SELECT postransaction.id, postransaction.datetime as TransDate, postransaction.info as TransReference,
    postransaction.confirmation as TransConfirmation, postransaction.expdate as TransExpDate,
    postransaction.amount as TransAmount, postransaction.transactionid as TransTransactionID,
    postransaction.authcode as TransAuthCode, postransaction.merchantactnum as TransMerchantAcctNum,
    postransaction.paymentmethodid, paymentmethod.name as TransPaymentMethod,
    so.num as SONum, so.salesman as SOSalesperson, so.billtoname as SOCustomer,
    sotype.name as SOType, company.name AS company

FROM postransaction, so, sotype, paymentmethod
    JOIN company ON company.id = 1

WHERE postransaction.soid = so.id
  AND so.typeid = sotype.id
  AND postransaction.paymentmethodid = paymentmethod.id
  AND postransaction.id = $P{posTransID}]]>
	</queryString>
