	<queryString>
		<![CDATA[SELECT userlog.eventdate, userlog.islogin, sysuser.username, company.name AS company

FROM userlog
    INNER JOIN sysuser ON userlog.userid = sysuser.id
    INNER JOIN company ON company.id = 1

WHERE sysuser.username LIKE $P{salesPersonUser}
  AND userlog.eventdate BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

ORDER BY $P!{cbSortBy}, userlog.eventdate]]>
	</queryString>
