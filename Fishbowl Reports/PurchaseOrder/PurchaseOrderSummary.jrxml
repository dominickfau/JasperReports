		<queryString>
			<![CDATA[SELECT SO.NUM AS ORDERNUMBER,
   SOITEM.QTYTOFULFILL - SOITEM.QTYFULFILLED AS QTY,
   UOM.CODE AS UOM,
   'SO' AS ORDERTYPE,
   SO.DATEFIRSTSHIP AS DATESCHEDULED

FROM so
   JOIN soitem ON so.id = soitem.soid
   JOIN uom ON soitem.uomid = uom.id
   JOIN product ON soitem.productid = product.id
   JOIN part ON product.partid = part.id

WHERE so.statusid > 10
   AND so.statusid < 60
   AND soitem.statusid < 50
   AND part.id = $P!{partID}
   AND $P!{ShowSalesOrders}
   AND $P!{poitemStatus} < 50

UNION ALL

SELECT WO.NUM AS ORDERNUMBER,
   WOITEM.QTYTARGET AS QTY,
   UOM.CODE AS UOM,
   'WO' AS ORDERTYPE,
   WO.DATESCHEDULED AS DATESCHEDULED

FROM wo
   JOIN woitem ON wo.id = woitem.woid
   JOIN uom ON woitem.uomid = uom.id

WHERE wo.statusid < 40
   AND woitem.partid = $P!{partID}
   AND woitem.typeid = 20
   AND $P!{ShowWorkOrders}
   AND $P!{poitemStatus} < 50

ORDER BY 5, 4, 1

-- rev 10/13/2020]]>
