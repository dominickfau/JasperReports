	<queryString>
		<![CDATA[SELECT ordertype.name AS ordertype, company.name AS company,
    COALESCE(so.num, po.num, xo.num) AS ordernum,
    ship.shiptoname AS shipto,
    COALESCE(po.dateissued, so.dateissued, xo.dateissued) AS orderdate,
    ship.dateshipped, carrier.name AS carrier, ship.id AS shipid


FROM ship
    LEFT JOIN po ON (po.id = ship.poid AND ship.ordertypeid=10)
    LEFT JOIN so ON (so.id = ship.soid AND ship.ordertypeid=20)
    LEFT JOIN xo ON (xo.id = ship.xoid AND ship.ordertypeid=40)
    LEFT JOIN carrier ON carrier.id = ship.carrierid
    LEFT JOIN ordertype ON ordertype.id = ship.ordertypeid
    JOIN company ON company.id = 1

WHERE ship.statusid = 30
  AND UPPER(carrier.name) LIKE UPPER($P{carrier})
  AND COALESCE(po.dateissued, so.dateissued, xo.dateissued) BETWEEN $P{dateRange1IssueDate} AND DATE_SUB($P{dateRange2IssueDate}, INTERVAL 1 SECOND)
  AND ship.dateShipped BETWEEN $P{dateRange1DateShipped} AND DATE_SUB($P{dateRange2DateShipped}, INTERVAL 1 SECOND)

ORDER BY $P!{sort}]]>
	</queryString>
