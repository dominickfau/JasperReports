	<queryString>
		<![CDATA[SELECT ship.ordertypeid AS ordertype, ship.soid AS soid, ship.xoid as xoid, ship.poid as poid, ship.billoflading AS bolnumber,
    ship.note AS shipnote, ordertype.name AS ordertypename, ship.cartoncount, ship.locationgroupid AS locationgroupid,
    carrier.name AS carriername, carrier.scac AS carrierscac,
    ship.shiptoname AS shipname, ship.shiptoaddress AS shipaddress,
    ship.shiptocity AS shipcity, stateconst.code AS shipstate,
    countryconst.name AS shipcountry, ship.shiptozip AS shipzip,
    COALESCE(so.num, po.num, xo.num) AS ordernumber,
    COALESCE(so.note, po.note, xo.note) AS ordernote,
    shipterms.name AS shipterms, fobpoint.name AS fobpoint,
    shipcarton.id AS cartonid, shipitem.qtyshipped, uom.code AS uomcode,
    COALESCE(soitem.productnum, poitem.partnum, xoitem.partnum) AS partnum,
    COALESCE(soitem.description, poitem.description, xoitem.description) AS partdescription,
    COALESCE(weightTable.weight, 0) AS cartonWeight,
    COALESCE(weightUom.code, 'lbs') AS weightUomCode,
    COALESCE(product.weight, popart.weight, xopart.weight) AS itemweight,
    COALESCE(productweightuom.code, popartweightuom.code, xopartweightuom.code) AS itemweightuom

FROM ship
    INNER JOIN shipitem ON ship.id = shipitem.shipid
    INNER JOIN carrier ON ship.carrierid = carrier.id
    INNER JOIN shipcarton ON (ship.id = shipcarton.shipid AND shipitem.shipcartonid = shipcarton.id)
    INNER JOIN (SELECT SUM(shipcarton.freightweight) AS weight, shipcarton.shipid
               FROM shipcarton
	     GROUP BY shipid) AS weightTable ON weightTable.shipid = ship.id
    INNER JOIN ordertype ON ship.ordertypeid = ordertype.id
    INNER JOIN uom ON shipitem.uomid = uom.id
    LEFT OUTER JOIN po ON (po.id = ship.poid AND ship.ordertypeid = 10)
    LEFT OUTER JOIN poitem ON (shipitem.poitemid = poitem.id AND ship.ordertypeid = 10)
    LEFT OUTER JOIN so ON (so.id = ship.soid AND ship.ordertypeid = 20)
    LEFT OUTER JOIN soitem ON (shipitem.soitemid = soitem.id AND ship.ordertypeid = 20)
    LEFT OUTER JOIN xo ON (xo.id = ship.xoid AND ship.ordertypeid = 40)
    LEFT OUTER JOIN xoitem ON (shipitem.xoitemid = xoitem.id AND ship.ordertypeid = 40)
    LEFT OUTER JOIN stateconst ON stateconst.id = ship.shiptostateid
    LEFT OUTER JOIN countryconst ON countryconst.id = ship.shiptocountryid
    LEFT OUTER JOIN shipterms ON shipterms.id = COALESCE(so.shiptermsid, po.shiptermsid, 0)
    LEFT OUTER JOIN fobpoint ON fobpoint.id = COALESCE(so.fobpointid, po.fobpointid, 0)
    LEFT OUTER JOIN sysProperties ON sysProperties.sysKey = 'Default Weight UomId'
    LEFT OUTER JOIN uom weightUom ON sysProperties.sysValue = weightUom.id
    LEFT OUTER JOIN part popart ON poitem.partid = popart.id
    LEFT OUTER JOIN part xopart ON xoitem.partid = xopart.id
    LEFT OUTER JOIN product ON soitem.productid = product.id
    LEFT OUTER JOIN uom popartweightuom ON popart.weightuomid = popartweightuom.id
    LEFT OUTER JOIN uom xopartweightuom ON xopart.weightuomid = xopartweightuom.id
    LEFT OUTER JOIN uom productweightuom ON product.weightuomid = productweightuom.id

WHERE ship.id = $P!{shipID}]]>
	</queryString>
