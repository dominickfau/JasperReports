	<queryString>
		<![CDATA[SELECT COSTLAYER.QTY AS QTY, COSTLAYER.ORGQTY, COSTLAYER.ORGTOTALCOST,
    COSTLAYER.TOTALCOST AS TOTALCOST, COSTLAYER.DATECREATED AS DATECREATED,
    PART.NUM AS PARTNUMBER, PART.DESCRIPTION AS PARTDESCRIPTION, COALESCE(ASACCOUNT.NAME,((SELECT asaccount.name AS qbaccount FROM sysproperties JOIN asaccount ON sysproperties.sysvalue = asaccount.id     WHERE syskey LIKE 'Acct ID inventoryAsset')),'Default Account') AS "InventoryAccount",
    COMPANY.NAME AS COMPANY, CURRENCY.SYMBOL, ((SELECT asaccount.name AS qbaccount FROM sysproperties JOIN asaccount ON sysproperties.sysvalue = asaccount.id     WHERE syskey LIKE 'Acct ID inventoryAsset')) AS DEFAULTACCOUNT, UOM.CODE UOM

FROM CostLayer
    LEFT JOIN Part ON part.id = costlayer.partid
    LEFT JOIN uom ON uom.id = part.uomid
    LEFT JOIN asaccount ON part.inventoryaccountid = asaccount.id
    JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1
    LEFT JOIN (SELECT asaccount.name AS qbaccount , asaccount.accountingid
		FROM sysproperties
		JOIN asaccount ON sysproperties.sysvalue = asaccount.id
		WHERE syskey LIKE 'Acct ID inventoryAsset') AS defaults ON defaults.accountingid > 0


WHERE costlayer.datecreated BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
  AND costlayer.statusid IN ($P!{ckShowActiveCostingLayers},$P!{ckShowFulfilledCostingLayers},$P!{ckShowVoidedCostingLayers})
  AND UPPER(part.num) LIKE UPPER($P{partNum})
  AND (UPPER(COALESCE(asaccount.name,defaults.qbaccount,'')) LIKE UPPER(CONCAT('%',CAST($P{AssetAccount} AS char(256)),'%')))

ORDER BY  (CASE
		WHEN $P{AssetAccount} NOT LIKE CAST('%' AS char(256)) THEN COALESCE(asaccount.name, defaults.qbaccount)
		ELSE part.num
	END),
	part.num ASC, costlayer.id, costlayer.datecreated]]>
	</queryString>
