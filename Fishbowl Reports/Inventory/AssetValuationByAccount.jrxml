	<queryString>
		<![CDATA[SELECT COALESCE(partcost.totalcost, 0) AS AverageTotalCost,
    (COALESCE(part.stdcost, 0) * COALESCE(partcost.qty, 0)) AS StandardTotalCost,
    SUM(COALESCE(costlayer.totalcost, 0)) AS LIFOFIFOTotalCost,
    Part.num AS PartNumber, Part.Description As PartDescription,
    COALESCE(asaccount.name, 'Use Default') AS "InventoryAccount",
    company.name AS company, currency.symbol

FROM Part
    LEFT JOIN partcost ON partcost.partid = part.id
    LEFT JOIN costlayer ON (costlayer.partid = part.id AND costlayer.statusid = 10)
    LEFT JOIN asaccount ON part.inventoryaccountid = asaccount.id
    JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1
    LEFT JOIN (SELECT SUM(qtyonhand.qty) AS qty, qtyonhand.partid AS partid
		FROM qtyonhand
		GROUP BY partid) AS qtyonhand ON qtyonhand.partid = part.id


WHERE part.typeid = 10
  AND UPPER(part.num) LIKE UPPER($P{partNum})
  AND UPPER(coalesce(asaccount.name, 'Use Default')) LIKE UPPER($P{AssetAccount})
  AND IF($P{HideInactive}=true,part.activeflag = 1,1=1)

GROUP BY 1,2,4,5,6,7, currency.symbol

HAVING IF($P{HideZeroQty}=1,SUM(qtyonhand.qty>0),1=1)

ORDER BY 6, part.num]]>
	</queryString>
