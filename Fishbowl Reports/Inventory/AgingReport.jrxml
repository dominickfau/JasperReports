	<queryString>
		<![CDATA[SELECT $P!{cbGroupBy} AS "Group",
    LOCATIONGROUP.NAME AS LOCATIONGROUP, PART.NUM AS PARTNUMBER,
    PART.DESCRIPTION AS PARTDESCRIPTION, LOCATION.NAME AS LOCATION,
    UOM.CODE AS UOMCODE, TAG.QTY AS QTY, COMPANY.NAME AS COMPANY,
    TAG.TRACKINGENCODING, TAG.DATECREATED AS DATECREATED,
    GROUP_CONCAT(CONCAT('<B>',TAGTRACKINGVIEW.ABBR,':</B> ',TAGTRACKINGVIEW.INFOFORMATTED), '<BR/>') AS TRACKING

FROM part
    INNER JOIN (
	SELECT MAX(id) AS id, COALESCE(SUM(qty), 0) AS qty, partID, locationID, trackingEncoding, min(Tag.datecreated) as datecreated, typeid
	FROM Tag
	GROUP BY trackingEncoding, partID, locationID, typeid
	) as tag ON part.id = tag.partid and tag.typeid > 10
    INNER JOIN location ON tag.locationid = location.id
    INNER JOIN locationgroup ON location.locationgroupid = locationgroup.id
    LEFT JOIN uom ON uom.id = part.uomid
    JOIN company ON company.id = 1
    LEFT JOIN tagtrackingview ON tagtrackingview.tagid = tag.id AND tagTrackingView.typeID <> 40

WHERE UPPER(part.num) LIKE UPPER($P{partNum})
  AND locationgroup.id IN ($P!{locationGroupID})
  AND location.id LIKE $P{locationID}
  AND part.id != 0
GROUP BY locationgroup, partnumber, partdescription, location, uomcode, company, trackingencoding, qty, datecreated

ORDER BY $P!{cbGroupBy}, location, part.num]]>
	</queryString>
